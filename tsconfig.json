{
    "compilerOptions": {
        "module": "Node16",
        "target": "ESNext",
        "lib": ["es6", "dom", "es2016", "es2017"],
        "sourceMap": true,
        "jsx": "react",
        "moduleResolution": "node16",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "resolveJsonModule": true
    },
    "include": ["media/js/src/**/*"],
    "exclude": [
        "node_modules",
        "dist"
    ]
}
